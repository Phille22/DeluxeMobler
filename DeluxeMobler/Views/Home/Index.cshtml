    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="Head">
    <div id="Inlogg">
        <p>Inloggad som: Phille</p>
        <a href="">Byt användare</a>
    </div>
        <h2>Deluxe Möbler!</h2>
        <ul>
            <li><a href="">Populära produkter</a></li>
            <li>
                <div class="Dropdown">
                    <a href="">Kategorier</a>
